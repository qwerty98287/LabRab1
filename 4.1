using namespace std;

int main()
{   int n,sum;
    cin >> n;
    int a[n];
    for(int i=0;i<n;i++){
        cin >> a[i];
    }
    cin >> sum;
    int i=0;
    int a1[n];
    while(sum>0){
    a1[i]=sum/a[i];
    sum=sum-a1[i]*a[i];
    if(i>n){
    cout<<'Error';
    exit(0);
    }
    i++;
  }
  int kol=0;
  for(int i=0;i<n;i++){
    cout<< a[i]<<'-'<<a1[i]<<', ';
    kol+=a1[i];
  }
  cout<<kol;
return(0);
}
